{
    "name": "webpress/dictionary",
    "authors": [{
        "name": "sonhaichu",
        "email": "sonhaichu@gmail.com"
    }],
    "autoload": {
        "classmap": ["src/app/"],
        "psr-4": {
            "VCComponent\\Laravel\\Dictionary\\": "src/app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VCComponent\\Laravel\\Dictionary\\Test\\": "tests/"
        }
    },
    "require": {
        "webpress/category-manager": "^3.1",
        "webpress/core": "^3.1",
        "webpress/configuration": "^3.1",
        "webpress/menu-manager": "^3.1",
        "webpress/script-manager": "^3.1",
        "webpress/tag-manager": "^3.1",
        "webpress/order-manager": "^3.1",
        "webpress/language": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.13",
        "orchestra/testbench": "5.0.0",
        "spatie/phpunit-watcher": "1.23.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "VCComponent\\Laravel\\Dictionary\\Providers\\DictionaryServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --color=always"
        ],
        "test:watch": [
            "vendor/bin/phpunit-watcher watch"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html coverages"
        ]
    }
}